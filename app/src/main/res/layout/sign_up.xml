<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:ignore="ExtraText">


    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:contentDescription="TODO"
        app:srcCompat="@drawable/group_6800"
        tools:ignore="ContentDescription,HardcodedText,MissingConstraints"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="16dp" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/poppins_bold"
        android:text="@string/create_your_account"
        android:textSize="20sp"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="97dp"
        tools:layout_editor_absoluteY="78dp" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="377dp"
        android:layout_height="46dp"
        android:background="@drawable/rounded_edit_text_background"
        android:fontFamily="@font/acme_regular"
        android:hint="@string/name"
        android:textColorHint="#2E7D32"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="136dp" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="377dp"
        android:layout_height="46dp"
        android:background="@drawable/rounded_edit_text_background"
        android:fontFamily="@font/acme_regular"
        android:hint="Email"
        android:textColorHint="#2E7D32"
        tools:ignore="HardcodedText,MissingConstraints"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="197dp" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="377dp"
        android:layout_height="46dp"
        android:background="@drawable/rounded_edit_text_background"
        android:fontFamily="@font/acme_regular"
        android:hint="Password"
        android:textColorHint="#2E7D32"
        tools:ignore="HardcodedText,MissingConstraints"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="266dp" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="377dp"
        android:layout_height="46dp"
        android:background="@drawable/rounded_edit_text_background"
        android:fontFamily="@font/acme_regular"
        android:hint="Confirm Pasword"
        android:textColorHint="#2E7D32"
        tools:ignore="HardcodedText,MissingConstraints"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="330dp" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/acme_regular"
        android:text="OR LOGIN WITH"
        tools:ignore="HardcodedText,MissingConstraints"
        tools:layout_editor_absoluteX="158dp"
        tools:layout_editor_absoluteY="391dp" />

    <Button
        android:id="@+id/imageButton"
        android:layout_width="371dp"
        android:layout_height="58dp"
        android:backgroundTint="@color/white"
        android:drawableStart="@drawable/google_icon"
        android:fontFamily="@font/acme_regular"
        android:text="CONTINUE WITH GOOGLE"
        android:textColor="@color/black"
        tools:ignore="HardcodedText,MissingConstraints"
        tools:layout_editor_absoluteX="25dp"
        tools:layout_editor_absoluteY="423dp" />


    <CheckBox
        android:id="@+id/checkBox"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/acme_regular"
        android:text="I have read the Privacy Policy"
        tools:ignore="HardcodedText,MissingConstraints"
        tools:layout_editor_absoluteX="25dp"
        tools:layout_editor_absoluteY="595dp" />

    <Button
        android:id="@+id/button7"
        android:layout_width="370dp"
        android:layout_height="56dp"
        android:fontFamily="@font/acme_regular"
        android:text="GET STARTED"
        android:textSize="16sp"
        tools:ignore="HardcodedText,MissingConstraints"
        tools:layout_editor_absoluteX="21dp"
        tools:layout_editor_absoluteY="653dp" />

</androidx.constraintlayout.widget.ConstraintLayout>